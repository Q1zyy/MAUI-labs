<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiApp1">


    <FlyoutItem Title="Home">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
    </FlyoutItem>  
    <FlyoutItem Title="Calculator">
        <ShellContent ContentTemplate="{DataTemplate local:CalculatorPage}" />
    </FlyoutItem>
    <FlyoutItem Title="ProgressBar">
        <ShellContent ContentTemplate="{DataTemplate local:ProgressBarPage}" />
    </FlyoutItem>
    <FlyoutItem Title="ORM">
        <ShellContent ContentTemplate="{DataTemplate local:Laba3}" />
    </FlyoutItem>
    <FlyoutItem Title="Converter">
        <ShellContent ContentTemplate="{DataTemplate local:ConverterPage}" Route="Laba3" />
    </FlyoutItem>
    

</Shell>
